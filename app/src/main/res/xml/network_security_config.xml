<?xml version="1.0" encoding="utf-8"?>
<!-- This config is about CLEARTEXT is not permitted (api 28) error on emulator.
Check manifest <application> tag to know that if this is in use. -->
<network-security-config>
    <base-config cleartextTrafficPermitted="true">
        <trust-anchors>
            <certificates src="system" />
        </trust-anchors>
    </base-config>
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">192.168.1.20</domain>
        <domain includeSubdomains="true">localhost</domain>
        <domain includeSubdomains="true">10.0.2.2</domain>
        <domain includeSubdomains="true">176.33.14.111</domain>

    </domain-config>
</network-security-config>